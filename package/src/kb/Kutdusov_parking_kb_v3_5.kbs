ПРАВИЛО Вызвать_парковочные_службы_1
ЕСЛИ
    ((Парковочные_службы.Тип_погоды) = ("Снегопад")) & (((Парковочные_службы.Состояние_погоды) = ("Средняя")) | ((Парковочные_службы.Состояние_погоды) = ("Сильная")))
ТО
    Парковочные_службы.Состояние_парковочной_службы = ("Едет_на_уборку_снега") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Цели_агента.Цель = ("Вызвать_парковочные_службы_для_уборки_снега") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
КОММЕНТАРИЙ Вызвать парковочные службы для уборки снега

ПРАВИЛО Отправление_Альтернативная_парковка
ЕСЛИ
    ((Парковка.Процент_заполнения) > (70)) & ((Альтернативная_парковка.Расстояние) < (2000))
ТО
    Транспортное_средство.Состояние_тс = ("Едет_на_альтернативную_парковку") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Цели_агента.Цель = ("Отправить_на_альтернативную_парковку") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
КОММЕНТАРИЙ Отправление_Альтернативная_парковка

