    Транспортное_средство.Состояние_тс = ("Едет_на_парковку") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Цели_агента.Цель = ("Отправить_на_парковку") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
