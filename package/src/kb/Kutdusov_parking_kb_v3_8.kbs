ПРАВИЛО Гарантированная_парковка
ЕСЛИ
    (Парковка.Процент_заполнения) < ("70")
ТО
    Цель_агента.Цель_агента = ("Отправить_на_парковку") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Транспортное_средство.Движение_ТС = ("На_парковку") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Транспортное_средство.Состояние_ТС = ("В_пути") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
КОММЕНТАРИЙ Гарантированная парковка ТС

ПРАВИЛО Вызов_ПС_из_за_нарушения_разметки
ЕСЛИ
    ((Транспортное_средство.Нарушение_парковки) = ("Задета_разметка")) & ((Парковка.Состояние_парковочного_места) = ("ПМ_нарушение_парковки"))
ТО
    Цель_агента.Цель_агента = ("Вызвать_парковочные_службы_из_за_неправильной_парковки") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Парковочные_службы.Состояние_ПС = ("ПС_в_пути") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
КОММЕНТАРИЙ Вызов_ПС_из_за_нарушения_разметки

ПРАВИЛО Вызов_ПС_из_за_неисправностей
