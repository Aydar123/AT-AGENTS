    (Парковка.Состояние_парковочного_места) = ("ПМ_разрушено")
ТО
    Цель_агента.Цель_агента = ("Вызвать_парковочные_службы_для_устранения_неисправностей") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
КОММЕНТАРИЙ Вызов_ПС_из_за_неисправностей

ПРАВИЛО Не_заезжать_пока_пожар
ЕСЛИ
    Выдача_жетона e Пожар
ТО
    Парковка.Шлагбаум = ("Закрыт") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Парковка.Кнопк_выдачи_или_забора_жетона = ("Выкл") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
    Парковка.Сигнал_светофора = ("Красный") УВЕРЕННОСТЬ [50; 100] ТОЧНОСТЬ 0
КОММЕНТАРИЙ Не_заезжать_пока_пожар

ПРАВИЛО Привести_ПС_в_готовность
